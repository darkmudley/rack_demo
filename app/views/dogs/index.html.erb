<html>
  <head>
    <title><%= @title || "Rack Demo" %></title>
  </head>

  <body>
    <div>
      <h1><%= @title %></h1>
      <p>
        <a href="/dogs/new">Create a new dog</a>
      </p>

      <ul>
        <% @dogs.each do |dog| %>
          <li>
            <%= dog.id %> -- 
            <a href="/dogs/<%= dog.id %>">
              <%= dog.name %>
            </a>
          </li>
        <% end %>
      </ul>
    </div>
  </body>
</html>
