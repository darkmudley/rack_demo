<html>
  <head>
    <title>Dogs</title>
  </head>

  <body>
    <div>
      <h1>Dogs</h1>
      <p>
        <code><%= @title %></code>
      </p>

      <ul>
        <% @dogs.each do |dog| %>
          <li>
            id: <%= dog.id %> -- name: <%= dog.name %>
          </li>
        <% end %>
      </ul>
    </div>
  </body>
</html>
